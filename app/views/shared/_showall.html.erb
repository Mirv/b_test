<ul>
    
    <% @o = object %>
    <% @o.each do |r| %>
    <% debug @o %>
        <% r.each do | showme | %>
        <li><%= showme.name %></li>
           <li><%= r.name %> 
           <%= link_to 'Show', r %>
           <%= link_to 'Edit', edit_region_path(r) %>
           <%= link_to 'Destroy', r, method: :delete, data: { confirm: 'Are you sure?' } %>

   <% end %> 
</ul>
  <% end %>  